{
  "manifest_version": 2,

  "name": "Notify",
  "description": "Notify from Hehehey.com",
  "version": "1.0.1",
  "icons": {
    "128": "icon.png"
  },

  "author": "DickRD",

  "browser_action": {},
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage",
    "http://*.hehehey.com/",
    "https://*.hehehey.com/"
  ]
}
